		{
			"name": "ðŸ–¼ï¸ Media Management (SECURED)",
			"item": [
				{
					"name": "Access Temporary Media (Valid)",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Generate expiration timestamp (1 hour from now)",
									"const expires = Math.floor(Date.now() / 1000) + 3600;",
									"pm.environment.set('expires_valid', expires);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}",
								"description": "Required for media access"
							}
						],
						"url": {
							"raw": "{{base_url}}/api/media/temp/test_image.jpg?expires={{expires_valid}}",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"media",
								"temp",
								"test_image.jpg"
							],
							"query": [
								{
									"key": "expires",
									"value": "{{expires_valid}}",
									"description": "Unix timestamp when link expires"
								}
							]
						},
						"description": "Access a temporary media file with valid authentication and expiration. Replace 'test_image.jpg' with actual filename from webhook."
					},
					"response": []
				},
				{
					"name": "Access Media - No Auth (Should Fail)",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"const expires = Math.floor(Date.now() / 1000) + 3600;",
									"pm.environment.set('expires_valid', expires);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Should return 401 Unauthorized', function () {",
									"    pm.response.to.have.status(401);",
									"});",
									"",
									"pm.test('Should contain auth error message', function () {",
									"    pm.expect(pm.response.text()).to.include('authorization');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{base_url}}/api/media/temp/test_image.jpg?expires={{expires_valid}}",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"media",
								"temp",
								"test_image.jpg"
							],
							"query": [
								{
									"key": "expires",
									"value": "{{expires_valid}}"
								}
							]
						},
						"description": "Test security: Access media without authentication (should fail with 401)."
					},
					"response": []
				},
				{
					"name": "Access Media - Expired Link (Should Fail)",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Generate expired timestamp (1 hour ago)",
									"const expired = Math.floor(Date.now() / 1000) - 3600;",
									"pm.environment.set('expires_expired', expired);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Should return 410 Gone', function () {",
									"    pm.response.to.have.status(410);",
									"});",
									"",
									"pm.test('Should contain expiration error message', function () {",
									"    pm.expect(pm.response.text()).to.include('expired');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{auth_token}}"
							}
						],
						"url": {
